{
    "name": "@jessejenks/ts-combinator",
    "version": "2.2.0",
    "description": "simple typescript combinator library",
    "main": "dist/index.js",
    "module": "dist/index.cjs.js",
    "source": "src/index.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jessejenks/ts-combinator.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "jest --watchAll",
        "build:js": "rollup --config",
        "build:types": "tsc --project ./ --emitDeclarationOnly",
        "build": "npm run build:types && npm run build:js",
        "doc": "typedoc"
    },
    "keywords": [
        "parser",
        "combinator",
        "typescript"
    ],
    "author": "Jesse Jenks",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "7.13.13",
        "@babel/preset-env": "7.13.12",
        "@babel/preset-typescript": "7.13.0",
        "@rollup/plugin-babel": "5.3.0",
        "@rollup/plugin-commonjs": "18.0.0",
        "@rollup/plugin-node-resolve": "11.2.1",
        "@types/jest": "26.0.20",
        "jest": "26.6.3",
        "rollup": "2.43.0",
        "ts-jest": "26.5.2",
        "typedoc": "^0.20.34",
        "typescript": "4.2.2"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ]
}
